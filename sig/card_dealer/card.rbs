module CardDealer
  class Card
    CLUBS: 'c'
    DIAMONDS: 'd'
    HEARTS: 'h'
    SPADES: 's'

    RANKS: Array[String]
    SUITS: Array[String]

    attr_reader rank: String
    attr_reader suit: String

    def initialize: (rank: String, suit: String | nil) -> void

    def to_s: () -> String
  end
end
